<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CActorUnit id="Beacon_Zerg">
        <MinimapTooltip value="Unit/Name/InfestedDominionHousing1"/>
        <MinimapIcon value="Assets\Textures\white.dds"/>
        <MinimapIconScale value="1.800000"/>
    </CActorUnit>
    <CActorModel id="ProtossPsiBroadcasterBuild" parent="ModelAdditionNoAnims">
        <Flags index="RespondsToUnitPlayerChange" value="1"/>
        <Inherits index="BaseModelScale" value="0"/>
        <Inherits index="Opacity" value="0"/>
        <Inherits index="Scale" value="0"/>
        <EditorCategories value=""/>
        <On Terms="UnitBirth.ShipGeneratorSmall.EditorPlaced" Send="Create"/>
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Stand,Build,Start Stand,Build Stand,Build,End 0 1.200000 AsTimeScale"/>
        <On Terms="ActorCreation" Send="TimerSet 0.000000 Start"/>
        <On Terms="ActorCreation" Send="TimerSet 13.000000 End"/>
        <On Terms="TimerExpired; TimerName Start" Target="_Selectable" Send="SetOpacity"/>
        <On Terms="TimerExpired; TimerName End" Target="_Selectable" Send="SetOpacity 1.000000 2.000000"/>
        <On Terms="TimerExpired; TimerName End" Send="AnimBracketStop BSD"/>
        <On Terms="AnimBracketState.*.AfterClosing" Send="Destroy"/>
        <On Terms="TimerExpired; TimerName Start" Send="Create ProtossBuildingStartSound"/>
        <HostSiteOps Ops="SOpShadow"/>
        <Model value="ProtossBuildingBirth"/>
        <CreepHeightClass value="Default"/>
    </CActorModel>
    <!--                                                                                         -->
    <!-- PsiIndoctrinator ...................................................................... -->
    <!--                                                                                         -->
    <CActorUnit id="PsiIndoctrinator">
        <On Terms="UnitConstruction.PsiIndoctrinator.Start" Send="Create PsiIndoctrinatorBuild"/>
    </CActorUnit>
    <CActorModel id="PsiIndoctrinatorBuild" parent="ModelAdditionNoAnims">
        <Aliases value="_TerranBuildModel"/>
        <Flags index="RespondsToUnitPlayerChange" value="1"/>
        <Inherits index="BaseModelScale" value="0"/>
        <Inherits index="Opacity" value="0"/>
        <Inherits index="Scale" value="0"/>
        <On Terms="UnitBirth.PsiIndoctrinator.EditorPlaced" Send="Create"/>
        <On Terms="ActorCreation" Send="AnimBracketStart AP_BuildA Birth,A,Start Birth,A,Stand"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName AP_BuildA" Send="AnimBracketStart AP_BuildB Birth,B,Start Birth,B,Stand"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName AP_BuildB" Send="AnimBracketStart AP_BuildC Birth,C,Start Birth,C,Stand"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName AP_BuildC" Send="AnimPlay AP_BuildD Birth,D,Start"/>
        <On Terms="AnimDone; AnimName AP_BuildD" Send="Destroy"/>
        <On Terms="Signal.*.A" Send="AnimBracketStop AP_BuildA"/>
        <On Terms="Signal.*.B" Send="AnimBracketStop AP_BuildB"/>
        <On Terms="Signal.*.C" Send="AnimBracketStop AP_BuildC"/>
        <On Terms="ActorCreation" Send="TimerSet 5.000000 A"/>
        <On Terms="ActorCreation" Send="TimerSet 10.000000 B"/>
        <On Terms="ActorCreation" Send="TimerSet 15.000000 C"/>
        <On Terms="TimerExpired; TimerName A" Send="Signal A"/>
        <On Terms="TimerExpired; TimerName B" Send="Signal B"/>
        <On Terms="TimerExpired; TimerName C" Send="Signal C"/>
        <On Terms="ActorCreation" Send="TimerSet 0.000000 Start"/>
        <On Terms="TimerExpired; TimerName Start" Target="_Unit" Send="SetScale 0.800000,0.800000,0.100000"/>
        <On Terms="TimerExpired; TimerName Start" Target="_Unit" Send="SetScale 1.000000 10.000000"/>
        <On Terms="TimerExpired; TimerName Start" Target="_Unit" Send="SetOpacity"/>
        <On Terms="TimerExpired; TimerName Start" Target="_Unit" Send="SetOpacity 1.000000 5.000000"/>
        <On Terms="TimerExpired; TimerName Start" Send="Create TerranConstructionSoundLevel0"/>
        <On Terms="TimerExpired; TimerName A" Send="Create TerranConstructionSoundLevel2"/>
        <On Terms="TimerExpired; TimerName B" Send="Create TerranConstructionSoundLevel4"/>
        <On Terms="TimerExpired; TimerName C" Send="Create TerranConstructionSoundLevel6"/>
        <HostSiteOps Ops="SOpShadow"/>
        <Model value="BunkerBuild"/>
        <Scale value="1.500000,1.500000,2.500000"/>
        <ModelFlags index="NeedsVolumeMonitoring" value="1"/>
        <ModelFlags index="OutlineOccluder" value="1"/>
        <CreepHeightClass value="Default"/>
    </CActorModel>
    <!--                                                                                         -->
    <!-- ShipGeneratorSmall                                                             -->
    <!--                                                                                         -->
    <CActorUnit id="ShipGeneratorSmall">
        <On Terms="UnitConstruction.ShipGeneratorSmall.Start" Send="Create ProtossPsiBroadcasterBuild"/>
    </CActorUnit>
    <CActorModel id="ShipGeneratorSmallBuild" parent="ProtossBuildingBuild"/>
</Catalog>
