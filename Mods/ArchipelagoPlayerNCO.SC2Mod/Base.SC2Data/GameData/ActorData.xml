<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CActorBeamSimple id="NovaTripWireBeam">
        <!-- Override -->
        <On Terms="Behavior.AP_FlashBangGrenadeNovaTripWire.On" Send="SetOpacity"/>
        <On Terms="Effect.AP_FlashBangGrenadeNovaTripWireRestartWarning.Start" Send="AnimPlay WorkEnd Stand,Work,End 0 0.000000 0.000000 2.000000 AsDuration"/>
        <On Terms="Effect.AP_FlashBangGrenadeNovaTripWireRestartWarning.Start" Send="SetOpacity 1.000000"/>
        <On Terms="Behavior.AP_FlashBangGrenadeNovaTripWire.Off" Send="AnimClear WorkEnd"/>
        <On Terms="Behavior.AP_FlashBangGrenadeNovaTripWire.Off" Send="SetOpacity 1.000000"/>
    </CActorBeamSimple>
</Catalog>