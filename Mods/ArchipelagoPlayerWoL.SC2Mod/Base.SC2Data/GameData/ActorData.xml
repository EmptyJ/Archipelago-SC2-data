<?xml version="1.0" encoding="UTF-8" ?>
<Catalog>
    <CActorProgress id="ProtossWarpIn">
        <!-- Override -->
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStart" Send="Create"/>
    </CActorProgress>
    <CActorModel id="WarpTrainModel">
        <!-- Override -->
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStart" Send="Create"/>
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStop" Send="AnimBracketStop BSD"/>
    </CActorModel>
    <CActorSound id="WarpTrainStart">
        <!-- Override -->
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStart" Send="Create"/>
    </CActorSound>
    <CActorSound id="WarpTrainLoop">
        <!-- Override -->
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStart" Send="Create"/>
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStop" Send="Destroy"/>
    </CActorSound>
    <CActorSound id="WarpTrainStop">
        <!-- Override -->
        <On Terms="Abil.AP_SuperWarpGateTrain.TargetStop" Send="Create"/>
    </CActorSound>
    <CActorUnit id="Odin">
        <GroupIcon>
            <Image index="0" value="AP\Assets\Textures\wireframe-terran-odin-mod.dds"/>
        </GroupIcon>
        <Wireframe>
            <Image index="0" value="AP\Assets\Textures\wireframe-terran-odin-mod.dds"/>
        </Wireframe>
        <WireframeShield>
            <Image value="AP\Assets\Textures\wireframe-terran-odin-shield01.dds"/>
            <Image value="AP\Assets\Textures\wireframe-terran-odin-shield02.dds"/>
            <Image value="AP\Assets\Textures\wireframe-terran-odin-shield03.dds"/>
        </WireframeShield>
        <On Terms="ActorCreation" Send="Create ShieldImpact"/>
    </CActorUnit>
    <CActorUnit id="TychusChaingun">
        <On Terms="ActorCreation" Send="Create ShieldImpact"/>
        <WireframeShield>
            <Image value="AP\Assets\Textures\wireframe-terran-marine-tychus-shield01.dds"/>
            <Image value="AP\Assets\Textures\wireframe-terran-marine-tychus-shield02.dds"/>
            <Image value="AP\Assets\Textures\wireframe-terran-marine-tychus-shield03.dds"/>
        </WireframeShield>
    </CActorUnit>
</Catalog>